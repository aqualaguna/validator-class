{
  "name": "validator-class",
  "version": "1.0.0",
  "description": "validator for json structure",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build:live",
    "clean": "rm -Rf lib",
    "build": "npm run clean && tsc -p .",
    "build:publish": "npm run build && npm publish",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test:docker": "docker-compose up -d && jest --runInBand",
    "test": "jest --runInBand",
    "test-watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aqualaguna/validator-class"
  },
  "keywords": [
    "nodejs",
    "js",
    "validator",
    "library",
    "jest"
  ],
  "author": "ricardo <ricardosentinel7@gmail.com>",
  "contributors": [
    "Ernesto <animusisis@yahoo.com>"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "typescript": "^3.5.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  }
}